<h1>Listing items</h1>

<table>
  <tr>
    <th>Typeitem</th>
    <th>Value</th>
    <th>ValueInfos</th>
    <th>Inc</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @items.each do |item| %>
  <tr>
    <td><%= item.typeItem %></td>
    <td><%= item.value %></td>
    <td><%= get4StoreNameElem(item.value) %></td>
    <td><%= item.inc %></td>
    <td><%= link_to 'Show', item %></td>
    <td><%= link_to 'Edit', edit_item_path(item) %></td>
    <td><%= link_to 'Destroy', item, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Item', new_item_path %>
